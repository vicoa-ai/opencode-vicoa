{
  "name": "@vicoa/opencode",
  "version": "0.1.1",
  "description": "OpenCode plugin for using OpenCode anywhere, on any device with Vicoa",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "opencode",
    "plugin",
    "vicoa",
    "ai",
    "coding",
    "agent",
    "mobile",
    "vibe coding",
    "dashboard"
  ],
  "author": "Vicoa",
  "license": "Apache-2.0",
  "homepage": "https://vicoa.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/vicoa-ai/opencode-vicoa.git"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^1.0.187"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
